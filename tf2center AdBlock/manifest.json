{
   "name": "TF2Center AdBlock",
   "description": "Remove Ads from tf2center.com",
   "permissions": [ "activeTab" ],
   "version": "1.0",
   "manifest_version": 2,
   
   "icons": {
      "128": "tf2center.png"
   },
   "browser_action": {
      "default_icon": {
         "38": "tf2center.png"
      },
      "default_title": "TF2Center AdBlock",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "removeads.js" ],
      "matches": [ "https://tf2center.com/*", "http://tf2center.com/*" ]
   } ]
}
